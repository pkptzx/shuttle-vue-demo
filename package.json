{
  "name": "myqr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -r \"vite\" \"cargo +1.65 shuttle run --working-directory ./API\"",
    "build": "vue-tsc && vite build --emptyOutDir",
    "build:all": "vue-tsc && vite build --emptyOutDir && cargo +1.65 build --manifest-path ./API/Cargo.toml",
    "shuttle:deploy": "pnpm build && cargo +1.65 shuttle deploy --working-directory ./API"
  },
  "dependencies": {
    "@vueuse/core": "^9.12.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "concurrently": "^7.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vue-tsc": "^1.1.2"
  }
}